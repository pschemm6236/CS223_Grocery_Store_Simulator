<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Point3D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SceneController">
   <children>
      <AnchorPane layoutX="0" layoutY="0" prefHeight="420.0" prefWidth="420.0">
         <children>
            <Label layoutX="153.0" layoutY="204.0" text="Results from simulations;" />
            <ButtonBar layoutX="-4.0" prefHeight="54.0" prefWidth="410.0">
               <buttons>
                  <Button mnemonicParsing="false" onAction="#switchToWelcomeScene" text="Welcome">
                     <font>
                        <Font size="9.0" />
                     </font>
                  </Button>
                  <Button layoutX="93.0" layoutY="18.0" mnemonicParsing="false" onAction="#switchToRecSettingScene" text="Rec Settings">
                     <font>
                        <Font size="9.0" />
                     </font>
                  </Button>
                  <Button layoutX="140.0" layoutY="21.0" mnemonicParsing="false" onAction="#switchToStartSimScene" text="Start Sim">
                     <font>
                        <Font size="9.0" />
                     </font>
                  </Button>
                  <Button layoutX="190.0" layoutY="21.0" mnemonicParsing="false" onAction="#switchToRunningSimsScene" text="Running Sims">
                     <font>
                        <Font size="9.0" />
                     </font>
                  </Button>
                  <Button layoutX="270.0" layoutY="21.0" mnemonicParsing="false" text="Sim Results">
                     <font>
                        <Font size="9.0" />
                     </font>
                  </Button>
               </buttons>
               <rotationAxis>
                  <Point3D />
               </rotationAxis>
            </ButtonBar>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
